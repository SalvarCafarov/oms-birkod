{
	"name": "ecarf-frontend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build:development": "tsc && vite build -m development",
		"build:production": "tsc && vite build -m production",
		"commit": "git-cz",
		"format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
		"lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
		"lint:fix": "npm run lint --fix",
		"prepare": "husky install",
		"preview": "vite preview",
		"start:dev": "vite --host",
		"test": "vitest",
		"type-check": "tsc"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx}": [
			"npm run format",
			"npm run lint:fix"
		],
		"package.json": "npx sort-package-json"
	},
	"config": {
		"commitizen": {
			"path": "@commitlint/cz-commitlint"
		}
	},
	"dependencies": {
		"@emotion/react": "11.14.0",
		"@emotion/styled": "11.14.0",
		"@hookform/resolvers": "^3.9.1",
		"@iconify/react": "4.1.1",
		"@mui/base": "^5.0.0-beta.66",
		"@mui/icons-material": "5.16.10",
		"@mui/lab": "^5.0.0-alpha.90",
		"@mui/material": "5.16.9",
		"@mui/x-data-grid": "6.20.4",
		"@reduxjs/toolkit": "1.9.7",
		"@tanstack/react-query": "5.62.7",
		"axios": "1.7.9",
		"dayjs": "^1.11.13",
		"formik": "2.4.6",
		"formik-mui": "5.0.0-alpha.1",
		"formik-mui-x-date-pickers": "0.0.1",
		"i18next": "23.16.8",
		"i18next-browser-languagedetector": "7.2.2",
		"i18next-chained-backend": "4.6.2",
		"i18next-http-backend": "2.7.1",
		"i18next-localstorage-backend": "4.2.0",
		"inquirer": "8.2.5",
		"luxon": "^3.5.0",
		"primeicons": "^7.0.0",
		"primereact": "10.8.5",
		"qs": "6.13.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-hook-form": "7.54.0",
		"react-hot-toast": "^2.4.1",
		"react-i18next": "13.5.0",
		"react-redux": "8.1.3",
		"react-router-dom": "6.28.0",
		"react-window": "^1.8.10",
		"recharts": "2.14.1",
		"yup": "1.5.0",
		"zod": "^3.24.1"
	},
	"devDependencies": {
		"@commitlint/cli": "18.6.1",
		"@commitlint/config-conventional": "18.6.3",
		"@commitlint/cz-commitlint": "18.6.1",
		"@testing-library/dom": "9.3.4",
		"@testing-library/react": "14.3.1",
		"@testing-library/user-event": "14.5.2",
		"@types/qs": "6.9.17",
		"@types/react": "18.3.16",
		"@types/react-dom": "18.3.5",
		"@typescript-eslint/eslint-plugin": "6.21.0",
		"@typescript-eslint/parser": "6.21.0",
		"@vitejs/plugin-react": "4.3.4",
		"commitizen": "4.3.1",
		"eslint": "8.57.1",
		"eslint-config-prettier": "9.1.0",
		"eslint-import-resolver-typescript": "3.7.0",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-prettier": "5.2.1",
		"eslint-plugin-react": "7.37.2",
		"eslint-plugin-react-hooks": "4.6.2",
		"eslint-plugin-simple-import-sort": "10.0.0",
		"husky": "8.0.3",
		"lint-staged": "15.2.11",
		"prettier": "3.4.2",
		"sort-package-json": "2.12.0",
		"typescript": "5.7.2",
		"vite": "4.5.5",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-svgr": "4.3.0",
		"vite-tsconfig-paths": "4.3.2",
		"vitest": "0.30.1"
	}
}
